<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="?colorSurfaceQuaternary" />
            <padding
                android:bottom="0dip"
                android:left="0dip"
                android:right="0dip"
                android:top="0dip" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:color="?colorSurface" />
        </shape>
    </item>
</layer-list>